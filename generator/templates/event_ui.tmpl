<?xml version="1.0" encoding="UTF-8"?>
<ui version="4.0">
 <class>{{ name }}Widget</class>
 <widget class="QWidget" name="{{ name }}Widget">
  <property name="geometry">
   <rect>
    <x>0</x>
    <y>0</y>
    <width>400</width>
    <height>300</height>
   </rect>
  </property>
  <property name="windowTitle">
   <string>{{ label }}</string>
  </property>
  <layout class="QFormLayout" name="formLayout">
{%- for arg in allargs -%}
{%- if arg.type == "string" %}
   <item row="{{ loop.index0 }}" column="0">
    <widget class="QLabel" name="label_string">
     <property name="text">
      <string>{{ arg.label }}:</string>
     </property>
    </widget>
   </item>
   <item row="{{ loop.index0 }}" column="1">
    <widget class="QLineEdit" name="string"/>
   </item>
{%- else %}
   <item row="{{ loop.index0 }}" column="0">
    <widget class="QLabel" name="label_arg{{ arg.index }}">
     <property name="text">
      <string>{{ arg.label }}:</string>
     </property>
    </widget>
   </item>
   <item row="{{ loop.index0 }}" column="1">
    <widget class="{{ arg|widget }}" name="arg{{ arg.index }}"/>
   </item>
{%- endif -%}
{%- endfor %}
  </layout>
 </widget>
 <customwidgets>
  <!--<customwidget>
   <class>RpgComboBox</class>
   <extends>QComboBox</extends>
   <header>/bla/blub/rpgcombobox.h</header>
	 </customwidget>-->
 </customwidgets>
 <resources/>
 <connections/>
</ui>
